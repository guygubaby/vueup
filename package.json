{
  "name": "@bryce-loskie/vueup",
  "version": "0.1.0",
  "description": "Vueup makes it easy to define vue lib build options for vite config",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "funding": "https://github.com/sponsors/guygubaby",
  "author": "guygubaby <guygubaby@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/guygubaby/vueup/issues"
  },
  "homepage": "https://github.com/guygubaby/vueup#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guygubaby/vueup.git"
  },
  "keywords": [],
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "tsup",
    "release": "bumpp --commit --push --tag",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "pnpm lint -- --fix"
  },
  "devDependencies": {
    "@bryce-loskie/eslint-config": "1.0.7",
    "@types/node": "17.0.23",
    "@vitejs/plugin-vue": "2.3.1",
    "@vitejs/plugin-vue-jsx": "1.3.8",
    "eslint": "8.11.0",
    "esno": "0.14.1",
    "tsup": "5.12.1",
    "typescript": "4.6.2",
    "vite": "2.8.6",
    "vitest": "0.7.10"
  },
  "peerDependencies": {
    "@vitejs/plugin-vue": "^2.2.4",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "vite": "^2.8.6"
  }
}
