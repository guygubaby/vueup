{
  "name": "@bryce-loskie/vueup",
  "version": "0.2.0",
  "description": "Vueup makes it easy to build vue component lib",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "type": "./dist/index.d.ts"
    }
  },
  "bin": {
    "vueup": "./bin/vueup.js"
  },
  "funding": "https://github.com/sponsors/guygubaby",
  "author": "guygubaby <guygubaby@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/guygubaby/vueup/issues"
  },
  "homepage": "https://github.com/guygubaby/vueup#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guygubaby/vueup.git"
  },
  "keywords": [],
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "tsup",
    "dev": "pnpm build -- --watch",
    "release": "bumpp --commit --push --tag",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "pnpm lint -- --fix"
  },
  "devDependencies": {
    "@bryce-loskie/eslint-config": "^1.0.7",
    "@types/minimist": "^1.2.2",
    "@types/node": "^17.0.22",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "bumpp": "^7.1.1",
    "eslint": "^8.11.0",
    "esno": "^0.14.1",
    "minimist": "^1.2.6",
    "picocolors": "^1.0.0",
    "tsup": "^5.12.1",
    "typescript": "^4.6.2",
    "unconfig": "^0.3.2",
    "vite": "^2.8.6",
    "vite-plugin-dts": "^1.0.5",
    "vitest": "^0.7.7"
  },
  "peerDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "@vitejs/plugin-vue-jsx": "^1.0.0",
    "typescript": "^4.0.0",
    "vite": "^2.0.0",
    "vite-plugin-dts": "^1.0.0"
  }
}
