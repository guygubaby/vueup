{
  "name": "@bryce-loskie/vueup",
  "version": "1.0.13",
  "description": "Vueup makes it easy to build vue component lib",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "type": "./dist/index.d.ts"
    }
  },
  "bin": {
    "vueup": "./bin/vueup.js"
  },
  "funding": "https://github.com/sponsors/guygubaby",
  "author": "guygubaby <guygubaby@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/guygubaby/vueup/issues"
  },
  "homepage": "https://github.com/guygubaby/vueup#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guygubaby/vueup.git"
  },
  "keywords": [],
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "tsup",
    "dev": "esno src/cli.ts -w",
    "release": "bumpp --commit --push --tag",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "pnpm lint -- --fix"
  },
  "devDependencies": {
    "@bryce-loskie/eslint-config": "1.0.14",
    "@types/minimist": "1.2.2",
    "@types/node": "17.0.45",
    "bumpp": "7.2.0",
    "eslint": "8.18.0",
    "esno": "0.14.1",
    "minimist": "1.2.6",
    "picocolors": "1.0.0",
    "sass": "1.52.3",
    "tsup": "5.12.9",
    "typescript": "4.7.4",
    "unconfig": "0.3.4",
    "vite": "2.9.12",
    "vitest": "0.8.4",
    "vue": "3.2.37"
  },
  "peerDependencies": {
    "typescript": "^4.0.0",
    "vite": "^2.0.0",
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "vite-plugin-dts": "^1.0.5"
  }
}
